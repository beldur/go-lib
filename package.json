{
  "name": "go-lib",
  "version": "0.1.2",
  "description": "A React Component that renders a GO Board",
  "main": "lib/index.js",
  "author": "Alexander LÃ¼cking <alexluecking@gmail.com>",
  "license": "MIT",
  "files": [
    "lib",
    "src",
    "dist"
  ],
  "scripts": {
    "clean": "rimraf lib dist",
    "build:commonjs": "babel src --out-dir lib --ignore *.test.js",
    "build:umd": "webpack",
    "build:umd:min": "cross-env NODE_ENV=production webpack -p",
    "build": "npm run build:commonjs && npm run build:umd && npm run build:umd:min",
    "prepublish": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "example": "webpack-dev-server --progress --content-base example"
  },
  "jest": {
    "verbose": true
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-loader": "^6.3.2",
    "babel-preset-env": "^1.1.11",
    "babel-preset-react": "^6.23.0",
    "cross-env": "^3.1.4",
    "enzyme": "^2.7.1",
    "jest": "^19.0.2",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "rimraf": "^2.6.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  }
}
